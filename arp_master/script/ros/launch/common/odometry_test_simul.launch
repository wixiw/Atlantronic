<launch>
	<include file="$(find arp_core)/script/ros/launch/machines.launch"/>
	<include file="$(find arp_master)/script/ros/launch/rosparam.launch"/>
	<node name="ArpOrocos" pkg="ocl" type="deployer-corba" args="-s $(find arp_master)/script/orocos/deployment/deploy_arp_master_simul.ops
		--" output="screen"
		respawn="false"
		required="true"
		/>
	<include file="$(find arp_hml)/script/ros/launch/arp_hml_ros_simul.launch"/>
	<!-- pas besoin pour le moment <include file="$(find arp_ods)/script/ros/launch/arp_ods_ros.launch"/>
		-->
	<!-- pas besoin pour le moment <include file="$(find arp_rlu)/script/ros/launch/arp_rlu_ros.launch"/>
		-->
	<node name="GraphicsSimu" pkg="arp_master" type="GraphicsSimuNode" args="/Ubiquity/simuRealPose"
		output="screen" launch-prefix="nice"/>
	<node name="OdometryTestingStratNode" pkg="arp_master" type="OdometryTestingStratNode.py"
		args="" output="screen" launch-prefix="xterm -e"
	respawn="false"
	required="true"
	/>
</launch>
