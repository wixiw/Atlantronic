<launch>
	<node 	name="StartupBashScript" 
		pkg="arp_core" 
		type="deployment_loading.sh"
		/>
	<include file="$(find arp_core)/script/ros/launch/machines.launch"/>
	<include file="$(find arp_master)/script/ros/launch/rosparam.launch"/>
	
	<node name="StratNode_Tanguy"   pkg="arp_master"  type="StratNode_Tanguy.py"     args="" output="screen"/>
	<node 	name="ArpOrocos" 
		pkg="ocl" 
		type="deployer-corba-gnulinux" 
		args="-s $(find arp_master)/script/orocos/deployment/deploy_arp_master.ops --" 
		output="screen"
		respawn="false"
		required="true"
		/>
	
	<include file="$(find arp_hml)/script/ros/launch/arp_hml_ros.launch"/>
	<include file="$(find arp_master)/script/ros/launch/arp_master_ros.launch"/>
</launch>