<!-- Use this file to simulate the Ubiquity platform hardware when you don't have access to the hardwares -->
<launch>
	<include file="$(find arp_core)/script/ros/launch/machines.launch"/>
	<include file="$(find arp_hml)/script/ros/launch/geometry_ubiquity.launch"/>
	<node 	name="ArpHml" 
			pkg="ocl" 
			type="deployer-corba-gnulinux" 
			args="-s $(find arp_hml)/script/orocos/deployment/deploy_ubiquity_simul.ops --" 
			output="screen"
			respawn="false"
			required="true"
			/>
	<node name="HmlGraphics"     pkg="arp_hml"  type="HmlGraphicsNode"     args="" output="screen" launch-prefix="nice"/>
</launch>
