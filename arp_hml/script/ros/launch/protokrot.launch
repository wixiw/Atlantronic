<!--  Use this file de deploy the real platform "Protokrot" hardware -->
<launch>
	<include file="$(find arp_core)/script/ros/launch/machines.launch"/>
	
	<node name="hokuyo" pkg="hokuyo_node" type="hokuyo_node" respawn="false" output="screen">
    	<param name="calibrate_time" type="bool" value="false"/> <!-- Starts up faster, but timestamps will be inaccurate. --> 
    	<param name="port" type="string" value="/dev/ttyACM0"/>  <!-- Set the port to connect to here -->
  		<param name="intensity" type="bool" value="false"/> <!-- ?? -->
  	</node>
  	
	<node 	name="ArpHml" 
			pkg="ocl" 
			type="deployer-gnulinux" 
			args="-s $(find arp_hml)/script/orocos/deployment/deploy_protokrot.ops --" 
			output="screen"
			/>
</launch>
